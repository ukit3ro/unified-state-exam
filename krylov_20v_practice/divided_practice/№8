""" from itertools import product
count = 0
for num in product('01234567', repeat=5):
    s = ''.join(num)
    if s[0] != '0':
        if s.count('4') == 2:
            if all(pair not in s for pair in '14 41 34 43 54 45 74 47'.split()):
                count += 1
print(count) """

